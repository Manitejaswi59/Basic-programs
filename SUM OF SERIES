//This program is written by taking some testcases 
#include<bits/stdc++.h>
using namespace std;
class Solution {
  public:
    long long seriesSum(long long n) {
         return (n*n+n)/2;
    }
};
int main(){
    int t;//T FOR NUMBER OF TEST CASES
    scanf("%d ",&t);
    while(t--){
        
        int n;
        scanf("%d",&n);
        
        Solution obj;
        long long res = obj.seriesSum(n);
        
        cout<<res<<endl;
        
    }
}
